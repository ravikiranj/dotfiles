# Custom Aliases added by ravikiranj dotfiles
alias vb="vim ~/.bashrc"
alias vv="vim ~/.vimrc"
alias so="source ~/.bashrc"
alias apt-get="sudo apt-get"
alias apt="sudo apt"
alias yum="sudo yum"
alias c="colordiff | less -R"
alias tcore="tmux attach -d -t core || tmux new -s core"
alias tkill="tmux kill-session -t "

# Custom Exports added by ravikiranj dotfiles
export PS1="[\u@\h \W]# "
